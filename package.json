  "devDependencies": {
    "concurrently": "^4.1.0",
    "html-loader": "^0.5.5",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.1",
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "node-sass": "^7.0.0",
    "pretty-checkbox": "^3.0.3",
    "react-codepen-embed": "^1.0.1",
    "react-spinners-kit": "^1.9.0",
    "routs": "^1.0.6"
  }
}
